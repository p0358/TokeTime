<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
	
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Settings</string>
			<key>footerText</key>
			<string>The image is shown on the lock screen at 4:20. If you enable the above setting, it will always be shown – useful for testing.</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>net.p0358.toketime</string>
			<key>key</key>
			<string>ShowAllTheTime</string>
			<key>label</key>
			<string>Always show</string>
			<key>PostNotification</key>
			<string>net.p0358.toketime/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Alignment</string>
			<key>footerText</key>
			<string>Fill is the default setting.</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSegmentCell</string>
			<key>defaults</key>
			<string>net.p0358.toketime</string>
			<key>key</key>
			<string>DisplayMode</string>
			<key>label</key>
			<string>Alignment</string>
			<key>default</key>
			<string>0</string>
			<key>validTitles</key>
			<array>
				<string>Fill</string>
				<string>Fit</string>
				<string>Bottom</string>
			</array>
			<key>validValues</key>
			<array>
				<string>0</string>
				<string>1</string>
				<string>2</string>
			</array>
			<key>PostNotification</key>
			<string>net.p0358.toketime/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Audio</string>
			<key>footerText</key>
			<string>Song „Dr. Dre – The Next Episode ft. Snoop Dogg, Kurupt, Nate Dogg” will be played while Snoop is shown on the lock screen and will stop when it stops being shown.
You can specify if you want the song to play from the beginning (or resume if it was paused), or to always start it at one of the predefined timestamps.
The song is not played if ringer is off.</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>net.p0358.toketime</string>
			<key>key</key>
			<string>AudioEnabled</string>
			<key>label</key>
			<string>Play</string>
			<key>PostNotification</key>
			<string>net.p0358.toketime/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSSegmentCell</string>
			<key>defaults</key>
			<string>net.p0358.toketime</string>
			<key>key</key>
			<string>AudioStartAt</string>
			<key>default</key>
			<string>0</string>
			<key>validTitles</key>
			<array>
				<string>0:00 or resume</string>
				<string>0:10</string>
			</array>
			<key>validValues</key>
			<array>
				<string>0</string>
				<string>1</string>
			</array>
			<key>PostNotification</key>
			<string>net.p0358.toketime/ReloadPrefs</string>
		</dict>

		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Developer</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Source code</string>
			<key>action</key>
			<string>openGitHub</string>
			<key>icon</key>
			<string>github.png</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Donate</string>
			<key>action</key>
			<string>openPayPal</string>
			<key>icon</key>
			<string>paypal.png</string>
		</dict>

	</array>
	<key>title</key>
	<string>TokeTime</string>
</dict>
</plist>
